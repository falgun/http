{
    "name": "falgunphp/http",
    "type": "library",
    "homepage": "https://github.com/falgun/http",
    "license": "MIT",
    "authors": [
        {
            "name": "S.M. Ataur Rahman",
            "email": "dev.ataur.rahman@gmail.com",
            "homepage": "https://ataur.net/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Falgun\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Falgun\\Http\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^0.12.18",
        "vimeo/psalm": "^4.8",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "phpunit": "phpunit --verbose --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}
